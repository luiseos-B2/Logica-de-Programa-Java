package vetor.pkg04;

import java.util.Arrays;


public class Vetor04 {

   
    public static void main(String[] args) {
        
        int num[]= {3,5,1,8,4};  
        for (int n:num){
            System.out.println(n);
        }
        int pos = Arrays.binarySearch(num,8);
        System.out.println("Encontrei o valor na posição " + pos);
    }
    
}
